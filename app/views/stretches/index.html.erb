<h1>All Suggested Stretches</h1>

<%= link_to "See Stretches by Body Area", body_areas_path %>

<% @stretches.each do |stretch| %>
  <div class="stretches-index-stretches">
    <div class="stretch-index-names">
      <h4 class="stretch-name"><%= link_to stretch.name, stretch_path(stretch) %></h4>
      <%= stretch.description %>
    </div>
      <%= render partial: "admin_buttons", :locals => {:stretch => stretch} %>
  </div>
<% end %>
