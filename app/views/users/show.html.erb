<h1><%= current_user.username %>'s Profile</h1>

<h3>Favorites</h3>

  <% current_user.favorites.each do |favorite| %>
    <div>
      <%= link_to favorite.stretch.name, stretch_path(favorite.stretch) %>
      <%= link_to "Remove from Favorites", user_favorite_path(current_user, favorite), method: "DELETE" %>
    </div>
  <% end %>
