<%= form_for [:admin, @stretch, @body_area] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :picture %>
  <%= f.text_field :picture %>

  <%= f.label :body_area_id %>
  <%= f.collection_select :body_area_id, BodyArea.all, :id, :name, prompt: 'Select a Body Area' %>

  <%= f.submit %>
<% end %>
